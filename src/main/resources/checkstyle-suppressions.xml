<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<suppressions>
    <suppress files=".*generated-test-sources.*" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*generated-sources.*" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*(?:Test|IT).java" checks="MagicNumberCheck"/>
    <suppress files=".*(?:Test|IT).java" checks="MagicNumber"/>
    <suppress files=".*(?:Test|IT).java" checks="Javadoc"/>
    <suppress files=".*(?:Test|IT).java" checks="Regexp"/>
    <suppress checks="MagicNumberCheck"
              files="Tests.java"/>
    <suppress checks="MagicNumberCheck"
              files="TestUtils.java"/>
</suppressions>

